<!DOCTYPE html>
<html>
<head>
  <title>Marynet</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>

	<% flash.each do |key, value| %>
		<div class="alert alert-<%= key %>"><%= value %></div>
	<% end %>

	<%= render 'layouts/header' %>

	<%= yield %>
	
	<%= render 'layouts/footer' %>

</body>
</html>
